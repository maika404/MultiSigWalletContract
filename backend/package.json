{
  "name": "multisigwalletcontract",
  "version": "1.0.0",
  "description": "MultiSigWalletを実装したスマートコントラクト用のリポジトリになります。",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"contract test start\" && truffle test",
    "build": "truffle build",
    "migrate:local": "truffle migrate --network local",
    "migrate:goreli": "truffle migrate --network goreli",
    "migrate:munbai": "truffle migrate --network munbai",
    "migrate:bsctestnet": "truffle migrate --network bsctestnet",
    "migrate:fuji": "truffle migrate --network fuji",
    "verify:goreli": "truffle run verify WalletFactory --network goreli",
    "verify:munbai": "truffle run verify WalletFactory --network munbai",
    "verify:bsctestnet": "truffle run verify WalletFactory --network bsctestnet",
    "verify:fuji": "truffle run verify WalletFactory --network fuji"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mashharuki/MultiSigWalletContract.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mashharuki/MultiSigWalletContract/issues"
  },
  "homepage": "https://github.com/mashharuki/MultiSigWalletContract#readme",
  "dependencies": {
    "@truffle/hdwallet-provider": "^2.0.11",
    "chai": "^4.3.6",
    "chai-bn": "^0.3.1",
    "dotenv": "^16.0.1",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.5.28"
  }
}
